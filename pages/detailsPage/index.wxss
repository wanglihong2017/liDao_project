.container_box {
  width: 100%;
}
.container_box video{
  width: 100%;
}
.container_box swiper {
  width: 100%;
}
.image_box {
  width: 96%;
  margin: 20rpx auto;
}
.image_box image {
  width: 100%;
}
.section_text {
  width: 96%;
  margin: 0 auto;
}
.section_text .title_text {
  padding: 20rpx 0;
}
.section_text .describe_text {
  padding: 0 0 20rpx;
  font-size: 26rpx;
  color: #bbbec4;
}
.serch_box {
  width: 96%;
  margin: 20rpx auto;
}
.serch_box_text{
  font-size: 26rpx;
  color: #bbbec4;
}
.serch_header {
  width: 66rpx;
  height: 66rpx;
  border-radius: 50%;
}
.serch_header image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.title_box {
  width: 70%;
  margin: 0 auto;
}
.sub_text {
  font-size: 24rpx;
  color: #bbbec4;
}
.comment_text{
  font-size: 26rpx;
  color: #999;
}
.get_box{
    width: 80rpx;
    text-align: center;
}
.reply_header{
    width: 50rpx;
    height: 50rpx;
}
.reply_header image{
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.reply_text{
    margin-left: 10rpx;
}
/* --------- */
.foots_input_box{
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 9999;
  background: #fff;
}
.prize_box{
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 8888;
}
.prize_box .serch_bos{
  width: 136rpx;
}
.prize_box .serch_bos text{
  display: inline-block;
  padding-left: 16rpx;
  font-size: 24rpx;
  color: #999;
}
.comment_list{
  padding-bottom: 140rpx;
}
.time_box{
  width: 96%;
  margin: 0 auto;
}
.time_box text{
  display: inline-block;
  padding-bottom: 10rpx;
  font-size: 20rpx;
  color: #999;
}
.van-divider{
  margin: 10rpx 0 !important;
}
.delete_box{
  margin-right: 40rpx;
}
